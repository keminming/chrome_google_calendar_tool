{
  "name": "Calendar++",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Add event to Google Calendar by Chrome Extension",
  "browser_action": {
    "default_title": "Open the popup",
    "default_icon": "icon-16.png",
    "default_popup" : "popup.html"
  },
  "icons": {
    "46" : "icon-46.jpg",
    "128" : "icon-128.png"
  },
  "background": 
  { 
	"scripts": ["contextMenus.js","calendar.js","scheduler.js"]
  },
  "key": "",
  "permissions": ["identity","*://apis.google.com/*","contextMenus","tabs"],
  "oauth2": {
    "client_id": "206855820890-ph06p7tufqse0hbdf8vmu36oohb6numf.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar"
    ]
  },
  "content_security_policy": "script-src 'self' https://*.google.com https://code.jquery.com/jquery-2.0.3.min.js; object-src 'self'"
}